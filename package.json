{
  "name": "ed25519-keygen",
  "version": "0.4.3",
  "description": "Generate ed25519 keys for SSH, PGP (GPG), TOR and SLIP-0010 hdkey",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "files": [
    "src/index.ts",
    "index.js",
    "index.d.ts",
    "index.d.ts.map",
    "src/hdkey.ts",
    "hdkey.js",
    "hdkey.d.ts",
    "hdkey.d.ts.map",
    "src/pgp.ts",
    "pgp.js",
    "pgp.d.ts",
    "pgp.d.ts.map",
    "src/ssh.ts",
    "ssh.js",
    "ssh.d.ts",
    "ssh.d.ts.map",
    "src/tor.ts",
    "tor.js",
    "tor.d.ts",
    "tor.d.ts.map",
    "src/utils.ts",
    "utils.js",
    "utils.d.ts",
    "utils.d.ts.map"
  ],
  "engines": {
    "node": "16.x || >= 18.0.0"
  },
  "dependencies": {
    "@noble/curves": "~1.0.0",
    "@noble/hashes": "~1.3.0",
    "@scure/base": "~1.1.0",
    "micro-packed": "^0.3.2"
  },
  "devDependencies": {
    "micro-should": "0.4.0",
    "prettier": "2.8.4",
    "typescript": "5.0.2"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./hdkey": {
      "types": "./hdkey.d.ts",
      "default": "./hdkey.js"
    },
    "./ssh": {
      "types": "./ssh.d.ts",
      "default": "./ssh.js"
    },
    "./pgp": {
      "types": "./pgp.d.ts",
      "default": "./pgp.js"
    },
    "./tor": {
      "types": "./tor.d.ts",
      "default": "./tor.js"
    },
    "./utils": {
      "types": "./utils.d.ts",
      "default": "./utils.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "prettier --check src",
    "format": "prettier --write 'src/**/*.{js,ts}' 'test/*.js'",
    "test": "node test/index.js"
  },
  "author": "Paul Miller (https://paulmillr.com)",
  "license": "MIT",
  "homepage": "https://github.com/paulmillr/ed25519-keygen",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulmillr/ed25519-keygen.git"
  },
  "keywords": [
    "ed25519",
    "PGP",
    "GPG",
    "RFC 4880",
    "RFC 6637",
    "SSH",
    "TOR",
    "onion",
    "key generation",
    "ec",
    "elliptic"
  ],
  "funding": [
    {
      "type": "individual",
      "url": "https://paulmillr.com/funding/"
    }
  ]
}
